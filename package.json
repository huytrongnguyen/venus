{
  "name": "venus",
  "version": "1.0.0",
  "description": "Venus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/huytrongnguyen/venus.git"
  },
  "bugs": "https://github.com/huytrongnguyen/venus/issues",
  "scripts": {
    "build": "npm run rosie-build && npm run admin-build",
    "rosie-build": "parcel build src/Shared/ClientApp/rosie/rosie.html --dist-dir src/Venus/wwwroot --no-source-maps --no-cache --no-autoinstall --no-content-hash",
    "admin-build": "parcel build src/Administration/ClientApp/admin.html --dist-dir src/Venus/wwwroot --no-source-maps --no-cache --no-autoinstall --no-content-hash",
    "admin-start": "parcel serve src/Administration/ClientApp/admin.html --dist-dir dist --no-cache -p 8585 --hmr-port 8586"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.7.2",
    "@popperjs/core": "2.11.8",
    "axios": "1.10.0",
    "bootstrap": "5.3.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "7.6.3"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.15.4",
    "@parcel/optimizer-esbuild": "2.15.4",
    "@types/bootstrap": "5.2.10",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/react-router-dom": "5.3.3",
    "buffer": "6.0.3",
    "parcel": "2.15.4",
    "process": "0.11.10",
    "typescript": "5.8.3"
  },
  "alias": {
    "administration/core": "./src/Administration/ClientApp/core",
    "rosie": "./src/Shared/ClientApp/rosie/ts"
  }
}